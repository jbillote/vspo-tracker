{
  "name": "vspo-tracker",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev ",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "npm install --package-lock-only --ignore-scripts"
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "turbo": "^2.3.3"
  },
  "dependencies": {
    "@types/bun": "latest"
  },
  "engines": {
    "node": ">=20.16.0"
  },
  "packageManager": "npm@10.8.1"
}
